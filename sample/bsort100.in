// Bubble sort

// Rules
use "sample/list_common.in";
use "sample/bsort.in";

// Execution
const LIST_ELEM=100;

MkList(r)~(LIST_ELEM);
BS(r1)~r;
r1;
free r1;

MkList(r)~(LIST_ELEM), Reverse(r1)~r;
BS(r2)~r1;
r2;
free r2;

MkRandList(r)~(LIST_ELEM);
BS(r1)~r;
r1;
free r1;

